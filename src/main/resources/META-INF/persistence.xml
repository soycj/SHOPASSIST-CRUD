<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="com.soycj.shopassist.crud_SHOPASSIST-CRUD" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.soycj.shopassist.crud.models.Items</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://10.0.0.9:3306/shopassist?zeroDateTimeBehavior=convertToNull"/>
